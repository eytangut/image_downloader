{
    "name": "Bulk Image Downloader",
    "description": "download every image on a web page",
    "version": "0.1",
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "js": ["content.js"],
            "matches": ["*://www.google.com/*"]   
        }
    ],
    "manifest_version": 2
}